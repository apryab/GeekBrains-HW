    "while True:\n",
    "    n = input('Введите ваше целое число ')\n",
    "    if n.isdigit():\n",
    "        break\n",
    "    else:\n",
    "        print('Вы ввели не число')\n",
    "n = int(n) \n",
    "n_decimal = n * 10\n",
    "n_hundred = n * 100\n",
    "answer = n + (n_decimal + n) + (n_hundred + n_decimal + n)\n",
    "print('Ваш ответ', answer)"
